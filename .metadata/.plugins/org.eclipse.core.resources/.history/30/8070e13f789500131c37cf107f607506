package RomanChiffre;

import org.junit.*;

public class RomanTest {

	Roman rom;
	
	@Before
	public void setUp() throws Exception {
		rom = new Roman();
	}

	@Test
	public void testI() {
		int lerom = rom.exec("I");
		System.out.println(lerom);
	}
	
	@Test
	public void testV() {
		int lerom = rom.exec("V");
		System.out.println(lerom);
	}
	
	@Test
	public void testIV() {
		int lerom = rom.exec("IV");
		System.out.println(lerom);
	}
	
	@Test
	public void testX() {
		int lerom = rom.exec("X");
		System.out.println(lerom);
	}
	
	@Test
	public void testIX() {
		int lerom = rom.exec("IX");
		System.out.println(lerom);
	}
	
	@Test
	public void testXV() {
		int lerom = rom.exec("XV");
		System.out.println(lerom);
	}

}
